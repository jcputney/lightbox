{
  "name": "bootstrap5-lightbox",
  "description": "A lightbox gallery plugin for Bootstrap 5 based on the modal plugin",
  "version": "1.0.0",
  "keywords": [
    "lightbox",
    "gallery",
    "bootstrap",
    "jquery",
    "modal"
  ],
  "homepage": "https://github.com/jcputney/lightbox",
  "author": "jcputney <jputney@noverant.com>",
  "main": "dist/bootstrap5-lightbox.min.js",
  "style": "dist/bootstrap5-lightbox.css",
  "less": "bootstrap5-lightbox.less",
  "repository": {
    "type": "git",
    "url": "https://github.com/jcputney/lightbox.git"
  },
  "bugs": {
    "url": "https://github.com/jcputney/lightbox/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js -p",
    "start": "NODE_ENV=development webpack-serve --port 8082 --open --no-clipboard --content ./ --config webpack.config.js --hot --colors --inline"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.13.2",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "cssnano": "^3.7.4",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-loader": "^2.1.4",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-serve": "^0.3.2"
  },
  "npmName": "bootstrap5-lightbox",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*"
      ]
    }
  ]
}
